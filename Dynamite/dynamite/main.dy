i32 printf(char* fmt, ...);

i32 main(i32 argc, char** argv)
{
	mut i32 x = 10;
	x = 20;

	mut i32& y = &x;
	y = 30;
	(y) = 40;

	x = 0;

	if (x)
	{
		printf("If\n");
	}
	else if (false)
	{
		printf("Else If\n");
	}
	else
	{
		printf("Else\n");
	}

	printf("Program is ending with exit code: %d\n", x);
	return x;
}